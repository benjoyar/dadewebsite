<div class="col-md-12">
  <table class="table table-hover">
    <thead>
      <tr>
        <td>#</td>
        <td>Title</td>
      </tr>
    </thead>
    <tbody>
      <% @messages.each do |message| %>
        <tr>
          <td>#</td>
          <td><%= link_to(message.title, message_path(message)) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
